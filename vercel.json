{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "env": {
    "VITE_AUTH0_DOMAIN": {
      "required": true
    },
    "VITE_AUTH0_CLIENT_ID": {
      "required": true
    },
    "VITE_AUTH0_AUDIENCE": {
      "required": true
    },
    "VITE_VULTR_API_URL": {
      "required": true
    }
  }
}
